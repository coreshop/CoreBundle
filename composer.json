{
  "name": "coreshop/core-bundle",
  "type": "symfony-bundle",
  "description": "CoreShop - Core Bundle (Glue Bundle)",
  "keywords": [
    "coreshop",
    "pimcore",
    "ecommerce"
  ],
  "homepage": "http://www.coreshop.org",
  "license": "GPLv3",
  "authors": [
    {
      "name": "Dominik Pfaffenbauer",
