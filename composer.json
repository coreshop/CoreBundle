    "coreshop/order-bundle": "dev-master",
    "coreshop/customer-bundle": "dev-master",
    "coreshop/product-bundle": "dev-master",
    "coreshop/address-bundle": "dev-master",
    "coreshop/currency-bundle": "dev-master",
    "coreshop/taxation-bundle": "dev-master",
    "coreshop/store-bundle": "dev-master",
    "coreshop/index-bundle": "dev-master",
    "coreshop/payment-bundle": "dev-master",
    "coreshop/sequence-bundle": "dev-master",
    "coreshop/payum-bundle": "dev-master",
